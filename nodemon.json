{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules", "dists"],
  "verbose": false,
  "exec": "ts-node --inspect -r tsconfig-paths/register ./src/index.ts",
  "events": {
    "restart":
      "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": ["src/"],
  "ext": "js json ts"
}
